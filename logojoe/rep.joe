#! /usr/bin/java com.veryant.joe.JavaObjectsExecutor

:args.
logo := !newInstance "Logo".
logo setTitle (args get 0).

*> logo setDelay 50.

logo penup ;
     setxy 80,-80
     setpensize 2
     pendown.
repcount := 1.
!while { repcount <= 1800},{
 logo right repcount
      forward 6
      left repcount
      forward 5
      right repcount
      right 0.3.
 repcount := repcount + 1.
}.
